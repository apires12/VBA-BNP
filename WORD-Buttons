
Sub ADTX()

Application.ScreenUpdating = False

Selection.InsertParagraphBefore
Selection.InsertBefore "[ADTXSTART]"
Selection.InsertAfter "[ADTXEND]"

Selection.InsertParagraphAfter

Application.ScreenUpdating = True

End Sub

